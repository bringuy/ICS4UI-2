import java.util.*;
public class telemarketer {

	public static void main(String[] args) {
	Scanner myInput = new Scanner(System.in);
	
	int digit1 = myInput.nextInt();
	int digit2 = myInput.nextInt();
	int digit3 = myInput.nextInt();
	int digit4 = myInput.nextInt();
	boolean telemarket = true;
	
	if (digit1 == 9|| digit1 ==8){
		telemarket= true;
	}
	else if (digit2==digit3|| digit3==digit2){
		telemarket = true;
	}
	else if (digit4==9||digit4==8){
		telemarket = true;
	}	
	if (telemarket = true){
		System.out.println("ignore");
	}
	else {
		telemarket = false;
		System.out.println("answer");
	}
	}

}
